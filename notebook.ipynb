   "source": [
    "import osmnx as ox\n",
    "import json\n",
    "\n",
    "# Descarga datos\n",
    "manresa_roads = ox.graph_from_place(\"Manresa, Spain\", network_type=\"drive\")\n",
    "nodes, edges = ox.graph_to_gdfs(manresa_roads)\n",
    "\n",
    "# Guarda en un JSON\n",
    "with open(\"manresa_roads.json\", \"w\") as f:\n",
    "    json.dump(json.loads(edges.to_json()), f)"
   ]
  }
